<CssModule name="Microsoft Extensions" standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx" pub-date="2012-02-23">

  <CssAtDirective name="-ms-viewport" version="3.0" browsers="IE10" include-root-properties="false" include-root-directives="false" syntax="@$(name) { width: device-width; }" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh868813.aspx">

    <CssProperty ref="width">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>

    <CssProperty ref="height">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>
  </CssAtDirective>

  <CssPseudo name="::-ms-browse" _locID="ms-browse-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于输入类型为文件控件的浏览按钮。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh779844.aspx"/>
  
  <CssPseudo name="::-ms-check" _locID="ms-check-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于复选框和单选按钮输入控件的选中。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465739.aspx"/>

  <CssPseudo name="::-ms-clear" _locID="ms-clear-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="当文本输入控件的输入区域不包含任何内容时，将一个或多个样式应用于该区域。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465740.aspx"/>

  <CssPseudo name="::-ms-expand" _locID="ms-expand-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于选择控件的下拉按钮。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465742.aspx"/>

  <CssPseudo name="::-ms-fill" _locID="ms-fill-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于进度条的条部分。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465757.aspx"/>

  <CssPseudo name="::-ms-fill-lower" _locID="ms-fill-lower-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块轨道中从其最小值直至 Thumb 当前选择的值的部分。在从左到右的布局中，这是滑块轨道在 Thumb 左侧的部分。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465745.aspx"/>

  <CssPseudo name="::-ms-fill-upper" _locID="ms-fill-upper-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块轨道中从 Thumb 当前选择的值直至滑块的最大值的部分。在从左到右的布局中，这是滑块轨道在 Thumb 右侧的部分。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465748.aspx"/>

  <CssPseudo name="::-ms-reveal" _locID="ms-reveal-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于输入类型为密码控件的密码显示按钮。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465773.aspx"/>

  <CssPseudo name="::-ms-thumb" _locID="ms-thumb-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于用户拖动的范围输入控件(也称为滑块控件)的部分。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465780.aspx"/>

  <CssPseudo name="::-ms-ticks-after" _locID="ms-ticks-after-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块中从 Thumb 之后开始直至滑块的最大值的刻度线。在从左到右的布局中，这些是 Thumb 右侧的刻度。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465789.aspx"/>

  <CssPseudo name="::-ms-ticks-before" _locID="ms-ticks-before-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块中表示其最小值直至 Thumb 当前选择的值的刻度线。在从左到右的布局中，这些是 Thumb 左侧的刻度。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465796.aspx"/>

  <CssPseudo name="::-ms-tooltip" _locID="ms-tooltip-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块(输入类型为范围)的工具提示。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465805.aspx"/>

  <CssPseudo name="::-ms-track" _locID="ms-track-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于滑块的轨道。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465813.aspx"/>

  <CssPseudo name="::-ms-value" _locID="ms-value-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="将一个或多个样式应用于文本/密码输入控件或选择控件的内容。" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465820.aspx"/>

  <CssPseudo name=":-ms-lang(en)" _locID="lang-pseudo-function" _locAttrData="description" version="3.0" browsers="IE10" syntax="html:-ms-lang(en) { color: blue; }" description="pseudo-class :-ms-lang(C) 表示 C 语言中的元素。" standard-reference="http://www.w3.org/TR/css3-selectors/#lang-pseudo"/>

  <CssPseudo name=":-ms-input-placeholder" version="3.0" browsers="IE10" syntax="input:-ms-input-placeholder { color: red; }" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh767367.aspx"/>

  <CssPseudo name=":-ms-keyboard-active" version="3.0" browsers="IE10" syntax="input:-ms-keyboard-active { background: red; }" standard-reference="#NoOnlineDocs" />

  <CssProperty _locID="zoom" _locAttrData="description" version="3.0" browsers="IE6,S4" restriction="enum, integer, number, percentage" syntax=".example { $(name): 1; }" description="设置或检索对象的放大比例。" standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx">

    <entry value="normal"/>
  </CssProperty>

  <CssProperty name="-ms-zoom" ref="zoom" browsers="IE8"/>

  <CssProperty _locID="filter" _locAttrData="description" version="3.0" browsers="IE6-9" restriction="" syntax="div { $(name): alpha(opacity = 50); }" description="仅限于 IE。用于生成视觉效果。" standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx"/>

  <CssProperty name="-ms-filter" ref="filter" restriction="string" syntax="div { $(name): 'alpha(opacity = 50)'; }" browsers="IE8-9"/>

  <CssProperty _locID="behavior" _locAttrData="description" version="3.0" browsers="IE6" type="url" restriction="url" syntax="div { $(name): url(http://example.com/png_fix.htc); }" description="仅限于 IE。用于扩展浏览器的行为" standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx"/>

  <CssProperty name="-ms-behavior" ref="behavior" browsers="IE8"/>

  <CssProperty _locID="-ms-accelerator" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="u { $(name): true; }" description="仅限于 IE。可以在按下 Alt 键前关闭快捷键的系统下划线" standard-reference="http://www.css3.com/css-accelerator/">

    <entry _locID="-ms-accelerator_true" _locAttrData="description" value="true" description="该元素包含一个快捷键序列。"/>
    <entry _locID="-ms-accelerator_false" _locAttrData="description" value="false" description="该元素不包含快捷键序列。"/>
  </CssProperty>

  <CssProperty _locID="-ms-line-break" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="p { $(name): strict; }" description="指定哪一组换行限制在元素中有效。" standard-reference="http://www.w3.org/TR/css3-text/#line-break0">

    <entry _locID="-ms-line-break_auto" _locAttrData="description" value="auto" description="UA 确定要用于 CJK 文字的一组换行限制，它可能根据行的长度更改这些限制；例如，对于较短的行，则使用限制性较低的一组换行规则。"/>
    <entry _locID="-ms-line-break_newspaper" _locAttrData="description" value="newspaper" description="使用限制最少的一组换行规则为 CJK 文字换行。通常用于较短的行，如报纸中。"/>
    <entry _locID="-ms-line-break_normal" _locAttrData="description" value="normal" description="使用一组正常的换行规则为 CJK 文字换行。"/>
    <entry _locID="-ms-line-break_strict" _locAttrData="description" value="strict" description="使用限制性高于“normal”的一组换行规则进行 CJK 文字换行。"/>
    <entry _locID="-ms-line-break_keep-all" _locAttrData="description" value="keep-all" description="CJK 字符的序列不能再在暗示的换行点处换行。此选项只应在词分隔符的存在仍会产生换行机会(如在朝鲜语中)的情况下使用。"/>
  </CssProperty>

  <!-- Text -->

  <CssProperty _locID="-ms-text-overflow" ref="text-overflow" browsers="IE10"/>

  <CssProperty _locID="-ms-text-underline-position" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="div { $(name): auto; }" description="设置在同一元素上指定的下划线的位置: 该位置不影响上级元素指定的下划线。此属性一般用于垂直书写的上下文(如日语文档)，其中，通常要求让下划线显示在受影响的文本的“上方”(右侧)" standard-reference="http://www.w3.org/TR/css3-text/#text-underline-position0">

    <entry _locID="-ms-text-underline-position_auto" _locAttrData="description" value="auto" description="用户代理可使用任何算法来确定下划线的位置。在水平行布局中，下划线应针对字母对齐。在垂直行布局中，如果语言设置为日语或朝鲜语，则下划线应针对上方对齐。"/>
    <entry _locID="-ms-text-underline-position_under" _locAttrData="description" value="under" description="下划线与元素的 em 框的下边缘(垂直书写模式下的左边缘)对齐。在这种情况下，下划线通常不会穿过下行字母。这有时称为“记账”下划线。"/>
    <entry _locID="-ms-text-underline-position_alphabetic" _locAttrData="description" value="alphabetic" description="下划线与字母基线对齐。在这种情况下，下划线可能穿过一些下行字母。"/>
    <entry _locID="-ms-text-underline-position_over" _locAttrData="description" value="over" description="下划线与元素的 em 框的边缘(垂直书写模式下的右边缘)对齐。在此模式下，上划线还会换侧。"/>
  </CssProperty>

  <CssProperty name="-ms-word-wrap" ref="word-wrap" version="3.0" browsers="IE8"/>

  <CssProperty _locID="-ms-word-break" ref="word-break" browsers="IE8"/>

  <CssProperty _locID="-ms-text-align-last" ref="text-align-last" browsers="IE8"/>

  <CssProperty _locID="-ms-text-justify" ref="text-justify" browsers="IE8"/>

  <CssProperty _locID="-ms-text-autospace" _locAttrData="description" version="3.0" browsers="IE8" restriction="enum" syntax="div { $(name): ideograph-numeric; }" description="确定在全宽标点符号字符显示在一行的开头时是否应剪裁它，以使其“墨迹”与上面和下面的行中的第一个字形对齐。" standard-reference="http://www.w3.org/TR/css3-text/#text-autospace">

    <entry _locID="-ms-text-autospace_none" _locAttrData="description" value="none" description="不创建额外的空间。"/>
    <entry _locID="-ms-text-autospace_ideograph-numeric" _locAttrData="description" value="ideograph-numeric" description="在一连串象形字母和数字字形之间创建 1/4em 的额外间距。"/>
    <entry _locID="-ms-text-autospace_ideograph-alpha" _locAttrData="description" value="ideograph-alpha" description="在一连串象形字母和非象形字母(如拉丁字母、西里尔字母、希腊字母、阿拉伯字母或希伯来字母)之间创建 1/4em 的额外间距。"/>
    <entry _locID="-ms-text-autospace_ideograph-space" _locAttrData="description" value="ideograph-space" description="扩展由象形文字围绕的空白字符的宽度。"/>
    <entry _locID="-ms-text-autospace_ideograph-parenthesis" _locAttrData="description" value="ideograph-parenthesis" description="在正常(非宽)括号与象形文字之间创建额外的间距。"/>
    <entry _locID="-ms-text-autospace_punctuation" _locAttrData="description" value="punctuation" description="根据特定于语言的排字约定的要求，创建额外的围绕标点的非换行间距。"/>
  </CssProperty>

  <!-- 3D transforms -->

  <CssProperty name="-ms-transform" ref="transform" browsers="IE9-9"/>

  <CssProperty name="-ms-transform-origin" ref="transform-origin" browsers="IE9-9"/>

  <CssProperty _locID="-ms-transform-origin-x" _locAttrData="description" version="3.0" browsers="IE10" restriction="length, percentage" description="应用于某个元素的转换原点的 x 坐标(相对于其边框)。" standard-reference="http://www.w3.org/TR/css3-3d-transforms/#transform-origin"/>

  <CssProperty _locID="-ms-transform-origin-y" _locAttrData="description" version="3.0" browsers="IE10" restriction="length, percentage" description="应用于某个元素的转换原点的 y 坐标(相对于其边框)。" standard-reference="http://www.w3.org/TR/css3-3d-transforms/#transform-origin"/>

  <CssProperty _locID="-ms-transform-origin-z" _locAttrData="description" version="3.0" browsers="IE10" restriction="length, percentage" description="应用于某个元素的转换原点的 z 坐标(相对于其边框)。" standard-reference="http://www.w3.org/TR/css3-3d-transforms/#transform-origin"/>

  <CssProperty _locID="-ms-block-progression" _locAttrData="description" version="3.0" browsers="IE8" restriction="enum" syntax="div { $(name): bt; }" description="“block-progression”属性设置 block-progression 值和流方向" standard-reference="http://www.w3.org/TR/2003/CR-css3-text-20030514/#Progression">

    <entry value="tb"/>
    <entry value="rl"/>
    <entry value="lr"/>
    <entry _locID="-ms-block-progression_bt" _locAttrData="description" value="bt" description="Bottom-to-top block flow. Layout is horizontal."/>
  </CssProperty>

  <CssProperty _locID="-ms-writing-mode" _locAttrData="description" version="3.0" browsers="IE8" restriction="enum" syntax="span { $(name): lr-tb; }" description="这是“direction”和“block-progression”的速记属性。" standard-reference="http://www.w3.org/TR/2003/CR-css3-text-20030514/#writing-mode">

