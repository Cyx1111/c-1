  <CssProperty _locID="-moz-box-orient" _locAttrData="description" version="3.0" browsers="FF1" restriction="enum" syntax="div { $(name): vertical; }" description="在 Mozilla 应用程序中，-moz-box-orient 用于指定一个框是水平还是垂直布局其内容。" standard-reference="https://developer.mozilla.org/en/CSS/-moz-box-orient">

    <entry _locID="-moz-box-orient_horizontal" _locAttrData="description" value="horizontal" description="该框在水平行中从左到右显示其子级。"/>
    <entry _locID="-moz-box-orient_vertical" _locAttrData="description" value="vertical" description="该框在垂直方向上从上到下层叠显示其子级。"/>
