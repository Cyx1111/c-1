  <CssProperty _locID="color" version="1.0" _locAttrData="description" type="color" restriction="color" description="元素的文本的颜色" syntax="body { $(name): red; }" standard-reference="http://www.w3.org/TR/css3-color/#foreground"/>

  <CssProperty _locID="opacity" version="3.0" browsers="C,FF3.6,IE9,O9,S1.2" _locAttrData="description" restriction="number(0-1)" description="元素的文本的不透明度，其中 1 表示不透明，0 表示完全透明。" syntax="article { $(name): opacity: 0.4; }" standard-reference="http://www.w3.org/TR/css3-color/#opacity"/>

