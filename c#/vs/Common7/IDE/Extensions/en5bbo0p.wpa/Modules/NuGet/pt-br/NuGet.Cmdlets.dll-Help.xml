  <!-- Open-PackagePage -->
  <command:command
      xmlns:maml="http://schemas.microsoft.com/maml/2004/10"
      xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10"
      xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">

    <command:details>
      <command:name>
        Open-PackagePage
      </command:name>
      <maml:description>
        <maml:para>
          Abrir o navegador apontando para ProjectUrl, LicenseUrl ou ReportAbuseUrl do pacote especificado.
        </maml:para>
      </maml:description>
      <command:verb>Open</command:verb>
      <command:noun>PackagePage</command:noun>
      <dev:version></dev:version>
    </command:details>

    <maml:description>
      <maml:para>
        Abrir o navegador apontando para ProjectUrl, LicenseUrl ou ReportAbuseUrl do pacote especificado.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Open-PackagePage</maml:name>
        <command:parameter required="true" position="named">
          <maml:name>Id</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Version</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Source</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>License</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>ReportAbuse</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>PassThru</maml:name>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="false" position="named">
        <maml:name>Id</maml:name>
        <maml:description>
          <maml:para>Especifica a ID do pacote a ser pesquisado.</maml:para>
        </maml:description>
        <command:parameterValue required="true">string</command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Version</maml:name>
        <maml:description>
          <maml:para>Especifica a versão do pacote a ser pesquisado. Se omitida, o padrão será a versão mais recente.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Source</maml:name>
        <maml:description>
          <maml:para>Especifica a origem do repositório no qual pesquisar o pacote. Se omitida, o padrão será a origem selecionada no controle suspenso da origem de pacote.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>License</maml:name>
        <maml:description>
          <maml:para>Indica que o cmdlet deve abrir a LicenseUrl do pacote especificado. Se nenhuma LicenseUrl ou ReportAbuseUrl estiver definida, o cmdlet abrirá a ProjectUrl por padrão.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>ReportAbuse</maml:name>
        <maml:description>
          <maml:para>Indica que o cmdlet deve abrir a ReportAbuseUrl do pacote especificado. Se nenhuma LicenseUrl ou ReportAbuseUrl for definida, o cmdlet abrirá a ProjectUrl por padrão.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>PassThru</maml:name>
        <maml:description>
          <maml:para>Se especificado, o cmdlet retornará o valor da URL solicitada.</maml:para>
        </maml:description>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  EXEMPLO 1 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject</dev:code>
        <dev:remarks>
          <maml:para>
Descrição
------------------------------------------------------------------
Abre um navegador para a URL do projeto especificada no pacote Ninject.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  EXEMPLO 2 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -License</dev:code>
        <dev:remarks>
          <maml:para>
Descrição
------------------------------------------------------------------
Abre um navegador para a URL da licença especificada no pacote Ninject.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  EXEMPLO 3 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -ReportAbuse</dev:code>
        <dev:remarks>
          <maml:para>
Descrição
------------------------------------------------------------------
Abre um navegador para a URL na origem de pacote atual usada para relatar abuso do pacote especificado.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  EXEMPLO 4 ----------</maml:title>
        <dev:code>PM&gt; $url = Open-PackagePage Ninject -License -WhatIf -PassThru</dev:code>
        <dev:remarks>
          <maml:para>
Descrição
------------------------------------------------------------------
Atribui a URL da licença à variável, $url, sem abrir a URL em um navegador.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
    
