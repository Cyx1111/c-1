<RegistrationEntries>
  <RegistrationEntry>
    <Type>ArchitectureTools</Type>
    <ChangeType>NoChange</ChangeType>
    <ServiceInterfaces />
    <ArtifactTypes>
      <ArtifactType>
        <Name>ArchitectureTools</Name>
        <OutboundLinkTypes />
      </ArtifactType>
    </ArtifactTypes>
  </RegistrationEntry>
  <RegistrationEntry>
    <Type>WorkItemTracking</Type>
    <ChangeType>NoChange</ChangeType>
    <ArtifactTypes>
      <ArtifactType>
        <Name>Workitem</Name>
        <OutboundLinkTypes>
          <OutboundLinkType>
            <Name>Model Link</Name>
            <TargetArtifactTypeTool>ArchitectureTools</TargetArtifactTypeTool>
            <TargetArtifactTypeName>ModelLink</TargetArtifactTypeName>
          </OutboundLinkType>
        </OutboundLinkTypes>
      </ArtifactType>
    </ArtifactTypes>
  </RegistrationEntry>    
</RegistrationEntries>