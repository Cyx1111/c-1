<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <_locDefinition xmlns="urn:locstudio">
    <_locDefault _loc="locNone"/>
    <_locTag _loc="locData">Title</_locTag>
    <_locTag _loc="locData">Description</_locTag>
    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
  </_locDefinition>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>创建用户定义类型</Title>
      <Shortcut></Shortcut>
      <Description>创建 CLR 集成用户定义类型。</Description>
      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>TypeName</ID>
          <ToolTip>类型的名称</ToolTip>
          <Default>Utf8String</Default>
        </Literal>
        <Literal>
          <ID>AssemblyName</ID>
          <ToolTip>程序集的名称</ToolTip>
          <Default>utf8string</Default>
        </Literal>
        <Literal>
          <ID>ClassName</ID>
          <ToolTip>程序集的类名称</ToolTip>
          <Default>Microsoft.Samples.SqlServer.utf8string</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL_SSDT">
        <![CDATA[
CREATE TYPE $TypeName$
EXTERNAL NAME $AssemblyName$.[$ClassName$];
]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
