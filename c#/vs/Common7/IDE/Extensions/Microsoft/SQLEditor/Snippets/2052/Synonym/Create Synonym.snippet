<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <_locDefinition xmlns="urn:locstudio">
    <_locDefault _loc="locNone"/>
    <_locTag _loc="locData">Title</_locTag>
    <_locTag _loc="locData">Description</_locTag>
    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
  </_locDefinition>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>创建同义词</Title>
      <Shortcut></Shortcut>
      <Description>创建同义词。</Description>
      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>SynonymName</ID>
          <ToolTip>同义词的名称</ToolTip>
          <Default>SynonymName</Default>
        </Literal>
        <Literal>
          <ID>DefaultSchemaName</ID>
          <ToolTip>架构名称</ToolTip>
          <Default>dbo</Default>
        </Literal>
        <Literal>
          <ID>SchemaName</ID>
          <ToolTip>对象架构的名称</ToolTip>
          <Default>SchemaName</Default>
        </Literal>
        <Literal>
          <ID>ObjectName</ID>
          <ToolTip>同义词的数据库对象的名称</ToolTip>
          <Default>object_name</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL_SSDT">
        <![CDATA[
CREATE SYNONYM $DefaultSchemaName$.$SynonymName$
    FOR $SchemaName$.$ObjectName$
]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
